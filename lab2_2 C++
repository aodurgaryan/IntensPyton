#include <iostream>
using namespace std;

int main() {
    double A, B, C;

    // Ввод коэффициентов A, B и C
    cout << "Введите коэффициенты A, B и C: ";
    cin >> A >> B >> C;

    // Проверка, проходит ли прямая через начало координат
    if (A * 0 + B * 0 + C == 0) {
        cout << "Прямая проходит через начало координат." << endl;
    } else {
        cout << "Прямая не проходит через начало координат." << endl;
    }

    return 0;
}
